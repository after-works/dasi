<% if voted?(song) %>
	<%= image_tag "voted.png"%>
<% else %>
	<%= form_for :vote, remote:true , :html=>{:class=>'form-inline', :id=>"vote-form", :'data-type'=>'script'}, :url=>votes_path do |f|%>
		<%= f.hidden_field :song_id, :value=>song.id%>
		<%= image_submit_tag "to_vote.png"%>
		<%= hidden_field :song, :id, :value=>song.id%>
	<% end %>
<% end %>