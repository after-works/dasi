<div>
	<!-- thumbnail -->
	<img src='<%= youtube_thumb(song) %>'/>
</div>
<div>
	<!-- vote btn -->
	<% if voted?(song) %>
		함
	<% else %>
		<%= form_for :vote, :html=>{:class=>'form-inline', :id=>"song-#{song.id}-vote-form", :'data-type'=>'json'}, :url=>votes_path do |f|%>
		<%= f.hidden_field :song_id, :value=>song.id%>
		<%= f.button "투표"%>
		<% end %>
	<% end %>

</div>
<div>
	<%= song.title %>
</div>
<div>
	<div>
		<!-- rank image-->
	</div>
	<div>
		<!-- vote num-->
		<%= song.votes.count%>
	</div>
</div>
