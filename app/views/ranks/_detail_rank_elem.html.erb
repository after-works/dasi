	<div class="thumb">
		<!-- thumbnail -->
		<a onclick="change_song('<%= song.youtube_id %>' , '<%= song.title %>' , '<%= song.user.name %>' , '<%= convert_time(song.created_at) %>')">
			<img src='<%= youtube_thumb(song) %>'/>
		</a>
	</div>
	<div class="vote" id="vote-<%=song.id%>">
		<!-- vote btn -->
		<%= render_vote_form(song) %>
	</div>
	<a onclick="change_song('<%= song.youtube_id %>' , '<%= song.title %>' , '<%= song.user.name %>' , '<%= convert_time(song.created_at) %>')">
		<div class="name">
			<%= song.title %>
		</div>
	</a>
	<div style="float:left;">
		<div class="ranks">
			<!-- rank image-->
		</div>
		<div class="voting-num">
			<!-- vote num-->
			<%= song.votes.count%>
		</div>
	</div>
