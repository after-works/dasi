<% if signed_in? %>
	<%= form_for comment_form do |f| %>
		<%= f.text_field( :context)%>
		<%= f.hidden_field :songtag_id, :value=>@songtag.id%>
		<%= f.hidden_field :uid, :value=>current_user.id%>
		<%= f.hidden_field :parent_cmt_id, :value=>parent_cmt.id%>
		<%= f.button %>
	<% end %>
<% end %>