<div id="detail-song-video">
	<iframe src="//www.youtube.com/embed/<%= song.youtube_id%>" frameborder="0" allowfullscreen></iframe>
</div>


<div class="info" id="detail-song-info">
	<div class="title">
		<%= song.title%>
	</div>
	<div class="small">
		<%= song.user.name%> | <%=convert_time(song.created_at)%> | <%= render 'facebook_utils/share_link_form', :songtag=>@songtag, :song=>song %>
	</div>
</div>
