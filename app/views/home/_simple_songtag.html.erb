<div id="main-content-elem-<%= songtag.id %>" class="simple-songtag-container">
	<div class="simple-songtag-title">	
		<!--<%= link_to "#{songtag.title}", "/songtags/#{songtag.id}", class:"simple-title-link" %>-->
		<a href="/songtags/<%= songtag.id %>"><span class="simple-title-link"><%= songtag.title %></span></a>
	</div>
	<div class="thumb"><a href="/songtags/<%= songtag.id %>"><img src=<%= youtube_thumb(song) %>></a></div>
	<div class="mint-title"><span class="rank-span">1ë“±</span><% song.title %></div>
	<div class="mint-info">
		<div class="space"></div>
		<div class="info-detail">
			<%= image_tag "stamp_main_15x15.png" %> <div class="info-num"><% song.votes.count %></div>
		</div>
		<div class="info-detail">
			<%= image_tag "music_main_13x15.png" %> <div class="info-num"><% songtag.songs.count %></div>
		</div>
		<div class="info-detail">
			<%= image_tag "reply_main_15x15.png" %> <div class="info-num"><% songtag.comments.count %></div>
		</div>

	</div>
</div>
